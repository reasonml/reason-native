{
  "name": "@reason-native/rely",
  "version": "0.0.0",
  "description": "A native Reason test runner that is heavily inspired by Jest",
  "repository": {
    "type": "git",
    "url": "https://github.com/facebookexperimental/reason-native/tree/master/src/rely"
  },
  "license": "MIT",
  "keywords": [
    "reason",
    "facebook",
    "jest",
    "runner",
    "test",
    "testing",
    "snapshot",
    "rely",
    "reason-native",
    "native"
  ],
  "esy": {
    "build": "esydune build",
    "buildDev": "refmterr esydune build",
    "install": "esydune install"
  },
  "scripts": {
    "test": "esy x TestRely.exe",
    "release": "node ./scripts/esy-prepublish.js rely.json"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": ">= 4.2.0  <= 4.7.0",
    "@reason-native/pastel": "*",
    "@reason-native/file-context-printer": "*",
    "esydune": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
